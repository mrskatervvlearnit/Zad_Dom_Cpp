#include <iostream>
using namespace std;
int main()
{ int x=0;
    bool tab[10][10];
    for(int i=0;i<10;i++)
    {
        for(int j=0;j<10;j++)
        {
            if(i==j)
            {
                tab[i][j]=true;
                x++;
            }
            else tab[i][j]=false;
            cout<<tab[i][j];
        }
        cout<<endl;
    }
    cout<<x;
}
